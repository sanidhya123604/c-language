//    Write a C program to check whether a number is divisible by 5 and 11 or not.

#include <stdio.h>

int main()
{
    int num;
    printf("enter the number:");
    scanf("%d",&num);
    if (num%5==0 && num%10==0)
    {
        printf("number is divided by 5 and 10");
    }
    else
    {
        printf("number is divided not  by 5 and 10");
    }
    

    return 0;
}
